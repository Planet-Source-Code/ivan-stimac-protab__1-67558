Private Sub txtIcHeig_Change()
    txtIcHeig.Text = Val(txtIcHeig.Text)
    txtIcHeig.SelLength = Len(txtIcHeig.Text)
    
    Changed = True
End Sub

Private Sub txtIcHeig_LostFocus()
    If Val(txtIcHeig.Text) < 4 Then
        txtIcHeig.Text = 4
        txtIcHeig.SelStart = 0
        txtIcHeig.SelLength = Len(txtIcHeig.Text)
    ElseIf Val(txtIcHeig.Text) > Val(txtTabHeigN.Text) Or Val(txtIcHeig.Text) > Val(txtTabHeigA.Text) Then
        txtIcHeig.Text = Val(txtTabHeigN.Text)
        txtIcHeig.SelStart = 0
        txtIcWid.SelLength = Len(txtIcWid.Text)
    End If
End Sub

