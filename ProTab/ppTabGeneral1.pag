Private Sub PropertyPage_ApplyChanges()
'    freezeRedraw = False
    mProTab.Orientation = cmbOrientation.ListIndex
    mProTab.StyleNormal = cmbStyleNormal.ListIndex
    mProTab.StyleActive = cmbStyleActive.ListIndex
    mProTab.TextAlignHorizontal = cmbAlignHor.ListIndex
    mProTab.TextAlignVertical = cmbAlignVert.ListIndex
    mProTab.ColorScheme = cmbColor.ListIndex
    
    mProTab.Appearance = cmbAppearance.ListIndex
    mProTab.ToolTipStyle = cmbTTip.ListIndex
    '
    mProTab.TabHeight = txtTabHeigN.Text
    mProTab.TabHeightActive = txtTabHeigA.Text
    '
    mProTab.IconWidth = txtIcWid.Text
    mProTab.IconHeight = txtIcHeig.Text
End Sub

Private Sub PropertyPage_SelectionChanged()
''''    Dim i As Integer
    If Not mProTab Is Nothing Then Exit Sub
    If TypeOf SelectedControls(0) Is ProTabCTL Then
        If mProTab Is Nothing Then
            GoTo LoadProperties
        End If
    End If
    Exit Sub
LoadProperties:
    fillCombos
    'freezeRedraw = True
    Set mProTab = SelectedControls(0)
    cmbOrientation.ListIndex = mProTab.Orientation
    cmbStyleNormal.ListIndex = mProTab.StyleNormal
    cmbStyleActive.ListIndex = mProTab.StyleActive
    cmbAlignHor.ListIndex = mProTab.TextAlignHorizontal
    cmbAlignVert.ListIndex = mProTab.TextAlignVertical
    cmbColor.ListIndex = mProTab.ColorScheme
    
    cmbAppearance.ListIndex = mProTab.Appearance
    cmbTTip.ListIndex = mProTab.ToolTipStyle
    '
    txtTabHeigN.Text = mProTab.TabHeight
    txtTabHeigA.Text = mProTab.TabHeightActive
    '
    txtIcWid.Text = mProTab.IconWidth
    txtIcHeig.Text = mProTab.IconHeight
    Changed = False
End Sub


